# Supabase credentials
# These can be found in your Supabase project's API settings
NEXT_PUBLIC_SUPABASE_URL=
NEXT_PUBLIC_SUPABASE_ANON_KEY=

# This is the service role key, keep it secret! Only used in Edge Functions.
# Find it in your Supabase project's API settings.
SUPABASE_SERVICE_ROLE_KEY=

# Stripe credentials for subscriptions
# Find these in your Stripe dashboard
STRIPE_SECRET_KEY=
STRIPE_WEBHOOK_SECRET=

# Secret for signing JWTs, if you use custom JWTs.
# Generate a long, random string.
JWT_SECRET=your-super-secret-jwt-token-with-at-least-32-characters

# Optional: Configuration for a third-party OCR service
# Example: 'google-vision', 'aws-textract'
OCR_PROVIDER=
OCR_API_KEY=
